{
  "name": "@jjangga0214/eslint-config",
  "version": "5.0.2",
  "description": "A sharable eslint config for javascript, typescript, and jest",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./javascript": {
      "import": "./dist/javascript.js",
      "types": "./dist/javascript.d.ts"
    },
    "./typescript": {
      "import": "./dist/typescript.js",
      "types": "./dist/typescript.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./jest": {
      "import": "./dist/jest.js",
      "types": "./dist/jest.d.ts"
    },
    "./commonjs": {
      "import": "./dist/commonjs.js",
      "types": "./dist/commonjs.d.ts"
    },
    "./helpers": {
      "import": "./dist/helpers.js",
      "types": "./dist/helpers.d.ts"
    },
    "./personal": {
      "import": "./dist/personal.js",
      "types": "./dist/personal.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@eslint/js": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "globals": "^13.24.0"
  },
  "peerDependencies": {
    "eslint": ">=8.2.0",
    "prettier": ">=3.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjangga0214/node-configs.git"
  },
  "bugs": {
    "url": "https://github.com/jjangga0214/node-configs/issues"
  },
  "homepage": "https://github.com/jjangga0214/node-configs/tree/main/packages/eslint-config",
  "author": {
    "name": "GIL B. Chan",
    "email": "bnbcmindnpass@gmail.com",
    "url": "https://github.com/jjangga0214"
  },
  "keywords": [
    "boilerplate",
    "config",
    "sharable",
    "env",
    "eslint",
    "eslintconfig",
    "flat",
    "flat config",
    "sharable config",
    "typescript",
    "jest",
    "react"
  ]
}
